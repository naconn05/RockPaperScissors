<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alex.rockpaperscissors" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".First_Selection"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DisplayResults"
            android:label="@string/title_activity_display_results"
            android:parentActivityName=".First_Selection" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.alex.rockpaperscissors.First_Selection" />
        </activity>
    </application>

</manifest>
